  "scripts": {
    "start": "cross-env NODE_ENV=development PORT=3000 BROWSER=none craco start",
    "start:netlify": "cross-env NODE_ENV=development PORT=3000 BROWSER=none craco start & netlify dev --port 8888",
    "build": "cross-env NODE_ENV=production CI=false GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true craco build",
    "build:prod": "cross-env NODE_ENV=production CI=false GENERATE_SOURCEMAP=false craco build",
    "test": "node server/tests/run-tests.js",
    "test:schema": "node server/tests/run-tests.js --suite schema",
    "test:api": "node server/tests/run-tests.js --suite api",
    "test:watch": "node server/tests/run-tests.js --watch",
    "eject": "react-scripts eject",
    "postinstall": "node -e \"try { require('fs').statSync('./node_modules/.bin/patch-package') && process.exit(0) } catch(e) { console.log('Skipping patch-package in CI environment'); process.exit(0) }\" && patch-package || true",
    "apply-patches": "patch-package",
    "update-docs": "node update-directory.js update",
    "watch-dir": "node update-directory.js watch",
    "record-changes": "node update-directory.js record",
    "deploy": "powershell -ExecutionPolicy Bypass -File ./deploy-netlify.ps1",
    "standardize-components": "node standardize-components.js",
    "debug": "start \"\" \"C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe\" --remote-debugging-port=9222 --user-data-dir=%TEMP%\\chrome-debug-profile && npm start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
