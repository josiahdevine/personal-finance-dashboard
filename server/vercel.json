{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "buildCommand": "npm run migrate:prod",
    "env": {
        "NODE_ENV": "production"
    }
} 