    "authcontext:update:dry": "node scripts/update-auth-imports.js --dry-run",
    "fix:craco-config": "node scripts/fix-craco-config.js",
    "fix:import-paths": "node scripts/fix-import-paths.js",
    "fix:missing-components": "node scripts/create-missing-components.js",
