    "db:migrate": "ts-node src/db/run-migrations.ts",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "dev:all": "concurrently \"cross-env PORT=3042 npm run dev\" \"netlify dev --port 8890\"",
    "component:analyze": "node scripts/standardize-component-structure.js",
    "component:fix": "node scripts/fix-component-structure.js",
    "component:fix:dry": "node scripts/fix-component-structure.js --dry-run",
